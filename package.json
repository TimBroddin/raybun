{
  "name": "raybun",
  "version": "0.1.0",
  "description": "A lightweight terminal-based viewer for Ray debug output",
  "author": "Tim Broddin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timbroddin/raybun.git"
  },
  "keywords": [
    "ray",
    "debug",
    "laravel",
    "php",
    "tui",
    "terminal",
    "cli",
    "spatie"
  ],
  "type": "module",
  "bin": {
    "raybun": "./src/index.tsx"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "bun --watch run src/index.tsx"
  },
  "dependencies": {
    "cli-highlight": "^2.1.11",
    "ink": "^6.6.0",
    "ink-ui": "^0.4.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
